if BACKLIGHT_LCD_SUPPORT

if BACKLIGHT_CLASS_DEVICE

config LCD_BIAS_TPS65132
	bool "Enble TPS65132 driver"
	help
	  define tps65132 driver config to enable tps65132 driver.
	  To set tps65132 driver which you choose avoid cannot driver OCP2131.
	  Please make sure CONFIG_MTK_LCM_BIAS_TPS65132 in xxxxxx_defconfig.mk.
	  And also make sure true on in ProjectConfig.mk

config BACKLIGHT_SGM37604
	bool "Enble SGM37604A driver"
	help
	  define SGM37604A driver config to enable SGM37604A driver.
	  To set SGM37604A driver which you choose avoid cannot driver SGM37604A.
	  Please make sure CONFIG_MTK_LCM_BACKLIGHT_SGM37604A in xxxxxx_defconfig.mk.
	  And also make sure true on in ProjectConfig.mk

endif

endif
